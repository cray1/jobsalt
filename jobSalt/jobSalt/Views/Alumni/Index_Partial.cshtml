@model jobSalt.Models.Feature.Alumni.AlumniPost[]
@for (int i = 0; i < Model.Count(); i++)
{
    var guid = Guid.NewGuid().ToString();
    <div class="container" style="padding-left:0px; padding-right:30px" data-toggle="collapse" data-parent="parentList" data-target="#@guid">
        <div class="well" style="position:relative">
            <h2>@Model[i].Name</h2>
            <h3>@Model[i].Company</h3>
            <div id="@guid" class="collapse">
                <b>Major: @Model[i].FieldOfStudy</b><br />
                <b>Location: @Model[i].Location.City, @Model[i].Location.State</b><br />
                <b>Email: @Model[i].Email</b><br />
                <b>Phone: @Model[i].PhoneNumber</b><br />
                <br />
            </div>
        </div>
    </div>
}

<script type="text/javascript">
    function StopPropagation(event) {
        event.stopPropagation();
    }
</script>